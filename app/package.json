    "@solana/spl-token": "^0.2.0",
    "@solana/wallet-adapter-wallets": "^0.15.5",
    "@solana/web3.js": "^1.39.1",
    "autoprefixer": "^10.4.5",
    "axios": "^0.27.1",
    "buffer": "^6.0.3",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.1",
    "node-sass": "^7.0.1",
    "npm": "^8.8.0",
    "postcss": "^8.4.12",
    "sass-loader": "^12.6.0",
    "solana-wallets-vue": "^0.3.2",
